{
    "model": {
        "name": "Linear Regression",
        "parameters": {
            "fit_intercept": true,
            "normalize": true
        }
    },
    "agent": {
        "type": "discrete",
        "parameters": {
            "action_space": ["buy", "sell", "hold"],
            "default_trade_size": 1000,
            "money_management": "fixed"
        }
    },
    "environment": {
        "type": "stocks",
        "parameters": {
            "lookback_period": 30,
            "transaction_cost": 0.01,
            "volume": 1000,
            "reward_function": "Sharpe Ratio",
            "symbol_list": ["TSLA"],
            "timeframe": "daily",
            "history": "last year",
            "default_trade_size": 1000
        }
    },
    "training": {
        "batch_size": 128,
        "num_epochs": 1,
        "optimizer": "None",
        "learning_rate": 0.0,
        "early_stopping": false,
        "patience": 0,
        "tolerance": 0.0,
        "max_grad_norm": 0.0,
        "logging_interval": 0,
        "checkpoint_interval": 0
    }
}
