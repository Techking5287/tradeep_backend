{
    "model": {
        "type": "DDPG",
        "parameters": {
            "input_shape": [10],
            "output_shape": [2],
            "layers": [64, 64],
            "discount_factor": 0.99
        }
    },
    "agent": {
        "type": "DDPG",
        "parameters": {
            "batch_size": 64,
            "buffer_size": 1000000,
            "actor_learning_rate": 0.0001,
            "critic_learning_rate": 0.001
        }
    },
    "environment": {
        "type": "stocks",
        "parameters": {
            "lookback_period": 30,
            "transaction_cost": 0.001,
            "volume": 1000,
            "reward_function": "Sharpe Ratio",
            "symbol_list": ["AAPL", "MSFT", "GOOG", "AMZN", "FB"]
        }
    }
}
